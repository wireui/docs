{
    "name": "wireui/docs",
    "description": "WireUi Documentation",
    "type": "library",
    "keywords": [
        "livewire components",
        "livewire-ui",
        "wireui",
        "laravel components",
        "blade components",
        "docs",
        "documentation"
    ],
    "repositories": {
        "torchlight/torchlight-laravel": {
            "type": "github",
            "url": "git@github.com:PH7-Jack/torchlight-laravel.git"
        }
    },
    "require": {
        "php": "^8.3",
        "laravel/framework": "^11.0",
        "torchlight/torchlight-laravel": "dev-main"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "orchestra/testbench": "^9.0",
        "pestphp/pest": "^2.24",
        "pestphp/pest-plugin-laravel": "^2.2",
        "pestphp/pest-plugin-livewire": "^2.1"
    },
    "authors": [
        {
            "name": "Pedro Oliveira",
            "email": "pedrolivertwd@gmail.com"
        },
        {
            "name": "Jo√£o Pedro Lopes",
            "email": "joaopalopes24@gmail.com"
        }
    ],
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "WireUi\\Docs\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "vendor/orchestra/testbench-core/laravel/app",
            "WireUi\\Docs\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "WireUi\\Docs\\ServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
